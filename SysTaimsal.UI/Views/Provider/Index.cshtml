@model IEnumerable<SysTaimsal.EL.Provider>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdProvider)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NameProvider)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IdProvider)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NameProvider)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IdProvider">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.IdProvider">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdProvider">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
